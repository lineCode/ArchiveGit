<UserControl x:Class="CharityRace.WelcomeView"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
	xmlns:c="clr-namespace:ClassLibrary"
>
	<Grid>
		<Grid Background="{StaticResource CommonBackground}">
			<ScrollViewer>
				<StackPanel MaxWidth="750">
					<c:Spacer MinHeight="20" />
					<c:HtmlTextBlock HtmlUri="Content/Welcome.html" />
					<c:Spacer MinHeight="200" />
				</StackPanel>
			</ScrollViewer>
		</Grid>
	</Grid>
</UserControl>
