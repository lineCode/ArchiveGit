POPUPTOOLS MENU
BEGIN
    POPUP "Mask"
    BEGIN
	MENUITEM ".", IDC_SQUARE,       MENUBREAK
	MENUITEM ".", IDC_CIRCLE,       MENUBREAK
	MENUITEM ".", IDC_FREE,         MENUBREAK
	MENUITEM ".", IDC_MAGICWAND,    MENUBREAK
	MENUITEM ".", IDC_SHIELD,       MENUBREAK
	MENUITEM ".", IDC_TRANSFORMER,  MENUBREAK
	MENUITEM ".", IDC_POINTER,      MENUBREAK
    END
    POPUP "Retouch"
    BEGIN
	MENUITEM ".", IDC_PAINT,        MENUBREAK
	MENUITEM ".", IDC_SPRAY,        MENUBREAK
	MENUITEM ".", IDC_CLONE,        MENUBREAK
	MENUITEM ".", IDC_TEXTURE,      MENUBREAK
	MENUITEM ".", IDC_SMEAR,        MENUBREAK
    END
    POPUP "Filter"
    BEGIN
	MENUITEM ".", IDC_SHARP,        MENUBREAK
	MENUITEM ".", IDC_SMOOTH,       MENUBREAK
	MENUITEM ".", IDC_LIGHTEN,      MENUBREAK
	MENUITEM ".", IDC_DARKEN,       MENUBREAK
    END
    POPUP "Fill"
    BEGIN
	MENUITEM ".", IDC_VIGNETTE,     MENUBREAK
	MENUITEM ".", IDC_TEXTUREFILL,  MENUBREAK
	MENUITEM ".", IDC_TINTFILL,     MENUBREAK
	MENUITEM ".", IDC_FLOOD,        MENUBREAK
    END
    POPUP "Draw"
    BEGIN
	MENUITEM ".", IDC_PENCIL,       MENUBREAK
	MENUITEM ".", IDC_DRAWSQUARE,   MENUBREAK
	MENUITEM ".", IDC_DRAWCIRCLE,   MENUBREAK
	MENUITEM ".", IDC_DRAWFREE,     MENUBREAK
    END
END

PP MENU
BEGIN
    POPUP       "&File"
    BEGIN
	MENUITEM        "&New...\t^N",          IDM_NEW
	MENUITEM        "&Open...\t^O",         IDM_OPEN
	MENUITEM        "&Save\t^S",            IDM_SAVE
	MENUITEM        "Save &as...\t^A",      IDM_SAVEAS
	MENUITEM        "Re&vert to Saved\t^V", IDM_REVERT
	MENUITEM        "Close",		IDM_CLOSE
	POPUP           "Recall"
	BEGIN
		MENUITEM "",                    IDM_RECALLIMAGE0
	END
	MENUITEM        SEPARATOR
	MENUITEM        "&Print...\t^P",        IDM_PRINT
	MENUITEM        "Scan...",              IDM_SCAN
	MENUITEM        "&Grab...\t^G",         IDM_GRAB
;       MENUITEM        "HandScan...",          IDM_HANDSCAN
	POPUP           "Setup",
	BEGIN
		MENUITEM "Printer...",          IDM_PRINTSETUP
		MENUITEM "Scanner...",          IDM_SCANSETUP
		MENUITEM "Grabber...",          IDM_GRABSETUP
	END
	MENUITEM        SEPARATOR
	MENUITEM        "E&xit\t^X",            IDM_EXIT
    END
    POPUP       "&Edit"
    BEGIN
	MENUITEM        "Undo\tAlt+Bksp",       IDM_UNDO
	MENUITEM        "Appl&y\t^Y",           IDM_APPLY
	MENUITEM        SEPARATOR
	MENUITEM        "Cut\tShift+Del",       IDM_CUT
	MENUITEM        "Copy\tCtrl+Ins",       IDM_COPY
	MENUITEM        "Copy to...",           IDM_COPYTOFILE
	MENUITEM        "Paste\tShift+Ins",     IDM_PASTE
	MENUITEM        "Paste from...",        IDM_PASTEFROMFILE
	MENUITEM        "Delete\tDel",          IDM_DELETE
	MENUITEM        SEPARATOR
	MENUITEM        "Show Paste Options",   IDM_PASTEOPTIONS
	MENUITEM        "Preferences...",       IDM_PREF
    END
    POPUP       "&Map"
    BEGIN
	MENUITEM        "Modify &Color Maps...\t^C",    IDM_MAP
	MENUITEM        "Contrast/Brightness...\t^J", IDM_CONTBRIT
	MENUITEM        "Posterize/&Threshold...\t^T", IDM_POSTERIZE
	MENUITEM        "&Hue/Saturation...\t^U",       IDM_HUE
	MENUITEM        "&Quartertones®...\t^Q",        IDM_QUARTERTONE
	MENUITEM        "Calibration...",       IDM_CALIBRATE
	MENUITEM        "Monitor gamma...",     IDM_MONITORMAP
    END
    POPUP       "M&ask"
    BEGIN
	MENUITEM        "&Remove Mask\t^R",     IDM_MASKREMOVE
	MENUITEM        "Mask entire image",    IDM_MASKALL
	MENUITEM        "Load Mask...",         IDM_MASKLOAD
	MENUITEM        "Save Mask...",         IDM_MASKSAVE
	MENUITEM        "Crop to Mask",         IDM_CROP
	MENUITEM        "&Blend Mask\t^B",      IDM_BLEND
    END
    POPUP       "Effect&s"
    BEGIN
	MENUITEM        "Size...",              IDM_SIZE
	POPUP           "Rotate",
	BEGIN
		MENUITEM "90° clockwise",       IDM_ROTATE90
		MENUITEM "90° counter clockwise",IDM_ROTATE270
		MENUITEM "180 degrees",         IDM_ROTATE180
		MENUITEM "Arbitrary angle...",  IDM_ROTATEANY
	END
	POPUP           "Mirror",
	BEGIN
		MENUITEM "Horizontal",          IDM_MIRRORX
		MENUITEM "Vertical",            IDM_MIRRORY
		MENUITEM "Diagonal",            IDM_MIRRORXY
	END
	MENUITEM        "Invert",               IDM_INVERT
	MENUITEM        "Split/Combine...",     IDM_SPLIT
	MENUITEM        "Color Reduction...",   IDM_REDUCTION
	MENUITEM        SEPARATOR
	MENUITEM        "Smooth...",            IDM_SMOOTH
	MENUITEM        "Sharpen...",           IDM_SHARPEN
	MENUITEM        "Edge Detection...",    IDM_EDGEDETECT
	MENUITEM        "Special...",           IDM_SPECIALFX
    END
    POPUP       "&Window"
    BEGIN
	MENUITEM        "Duplicate &Window\t^W",IDM_NEWWINDOW
	MENUITEM	"Rulers", IDM_RULERS
	POPUP           "Show/Hide",
	BEGIN
		MENUITEM "Show Scratchpad"      IDM_SCRATCHPAD
		MENUITEM "Show Tools",          IDM_TOOLSHOW
		MENUITEM "Show Status",         IDM_STATUSSHOW
;               MENUITEM "Show Color Palette",  IDM_PALETTESHOW
;               MENUITEM "Show Color Shields",  IDM_SHIELDSHOW
;               MENUITEM "Show Clipboards",     IDM_CLIPBOARDSHOW
	END
	POPUP           "Arrange",
	BEGIN
		MENUITEM "Arrange icon windows", IDM_ARRANGEICONS
		MENUITEM "Cascade windows",     IDM_CASCADE
		MENUITEM "Tile windows",        IDM_TILE
	END
	MENUITEM        "Close All",            IDM_CLOSEALL
    END
    POPUP       "&Help"
    BEGIN
	MENUITEM        "&Index\tF1",           IDM_HELP_INDEX
	MENUITEM        "Context &Sensitive\tShift+F1", IDM_HELP_PROCEDURES
	MENUITEM        "&Toolbox",             IDM_HELP_TOOLBOX
	MENUITEM        "&Keyboard",            IDM_HELP_KEYBOARD
	MENUITEM        "&Commands",            IDM_HELP_COMMANDS
	MENUITEM        "&Using Help",          IDM_HELP_INTRO
;       MENUITEM        "&Tutorial...\t^T",     IDM_TUTORIAL
	MENUITEM        SEPARATOR
	MENUITEM        "About...",             IDM_ABOUT
    END
END

