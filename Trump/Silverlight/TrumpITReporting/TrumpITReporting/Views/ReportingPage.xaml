										<c:StretchPanel Margin="12,15,5,0">
											<RadioButton Style="{StaticResource TrumpRadioButtonLeft}" GroupName="SummaryButtons" x:Name="x_TopLeadsButton" Content="TOP LEADS" IsChecked="True" />
											<c:Spacer MinHeight="13" />
											<RadioButton Style="{StaticResource TrumpRadioButtonLeft}" GroupName="SummaryButtons" x:Name="x_SuccessButton" Content="SUCCESS" />
											<c:Spacer MinHeight="13" />
											<RadioButton Style="{StaticResource TrumpRadioButtonLeft}" GroupName="SummaryButtons" x:Name="x_RevenueButton" Content="REVENUE" />
											<c:Spacer MinHeight="13" />
											<RadioButton Style="{StaticResource TrumpRadioButtonLeft}" GroupName="SummaryButtons" x:Name="x_CostButton" Content="COST" />
											<c:Spacer MinHeight="13" />
											<RadioButton Style="{StaticResource TrumpRadioButtonLeft}" GroupName="SummaryButtons" x:Name="x_InvitationsButton" Content="INVITATIONS" />
											<c:Spacer MinHeight="13" />
											<RadioButton Style="{StaticResource TrumpRadioButtonLeft}" GroupName="SummaryButtons" x:Name="x_ActivityMapButton" Content="ACTIVITY MAP" />
										</c:StretchPanel>
										<Grid c:StretchPanel.Length="*">
											<Grid Visibility="{Binding IsChecked, ElementName=x_TopLeadsButton, Converter={StaticResource BoolToVisible}}">
												<app:PersonsPanel Margin="5" />
											</Grid>
											<Grid Visibility="{Binding IsChecked, ElementName=x_SuccessButton, Converter={StaticResource BoolToVisible}}">
												<c:StretchPanel>
													<c:StretchPanel Orientation="Horizontal" Margin="5">
														<TextBlock Text="Success Reporting: " FontSize="18" />
														<TextBlock Text="{Binding Name}" FontSize="20" Foreground="CornflowerBlue" />
													</c:StretchPanel>
													<c:StretchPanel Orientation="Horizontal" c:StretchPanel.Length="*">
														<c:StretchPanel Margin="5,0,5,5" MinWidth="200">
															<TextBlock Text=" " />
															<c:StretchPanel Orientation="Horizontal">
																<TextBlock Text="Total Profit: " />
																<TextBlock Text="{Binding SuccessTotal, Converter={StaticResource StringFormatter}, ConverterParameter='\{0:c}'}" Foreground="CornflowerBlue" />
															</c:StretchPanel>
															<TextBlock Text=" " />
															<TextBlock Text="Profit Breakdown" FontSize="14" FontWeight="Bold" />
															<TextBlock Text=" " />
															<ItemsControl ItemsSource="{Binding SuccessData}" IsTabStop="False">
																<ItemsControl.ItemTemplate>
																	<DataTemplate>
																		<c:StretchPanel Orientation="Horizontal">
																			<TextBlock Style="{StaticResource CommonTextBlock}" Text="{Binding Name, Converter={StaticResource StringFormatter}, ConverterParameter='\{0}: '}" />
																			<TextBlock Style="{StaticResource CommonTextBlock}" Text="{Binding Value, Converter={StaticResource StringFormatter}, ConverterParameter='\{0:c}'}" Foreground="CornflowerBlue" />
																		</c:StretchPanel>
																	</DataTemplate>
																</ItemsControl.ItemTemplate>
															</ItemsControl>
														</c:StretchPanel>
														<c:StretchPanel Margin="0,0,5,5" c:StretchPanel.Length="*">
															<Charting:Chart LegendStyle="{StaticResource HiddenLegend}" c:StretchPanel.Length="*" MinWidth="100" MinHeight="100">
																<Charting:Chart.Series>
																	<Charting:ColumnSeries ItemsSource="{Binding SuccessData}" IndependentValueBinding="{Binding Name}" DependentValueBinding="{Binding Value}" />
																</Charting:Chart.Series>
															</Charting:Chart>
															<!--<telerikChart:RadChart ItemsSource="{Binding SuccessData}" Background="Transparent" c:StretchPanel.Length="*" />-->
														</c:StretchPanel>
													</c:StretchPanel>
												</c:StretchPanel>
											</Grid>
											<Grid Visibility="{Binding IsChecked, ElementName=x_RevenueButton, Converter={StaticResource BoolToVisible}}">
												<c:StretchPanel>
													<c:StretchPanel Orientation="Horizontal" Margin="5">
														<TextBlock Text="Revenue Reporting: " FontSize="18" />
														<TextBlock Text="{Binding Name}" FontSize="20" Foreground="CornflowerBlue" />
													</c:StretchPanel>
													<c:StretchPanel Orientation="Horizontal" c:StretchPanel.Length="*">
														<c:StretchPanel Margin="5,0,5,5" MinWidth="200">
															<TextBlock Text=" " />
															<c:StretchPanel Orientation="Horizontal">
																<TextBlock Text="Total Revenue: " />
																<TextBlock Text="{Binding RevenueTotal, Converter={StaticResource StringFormatter}, ConverterParameter='\{0:c}'}" Foreground="CornflowerBlue" />
															</c:StretchPanel>
															<TextBlock Text=" " />
															<TextBlock Text="Revenue Breakdown" FontSize="14" FontWeight="Bold" />
															<TextBlock Text=" " />
