											</Grid>
											<Grid Visibility="{Binding IsChecked, ElementName=x_ActivityMapButton, Converter={StaticResource BoolToVisible}}">
												<c:StretchPanel>
													<c:StretchPanel Orientation="Horizontal" Margin="5">
														<TextBlock Text="Activity Map Reporting: " FontSize="18" />
														<TextBlock Text="{Binding Name}" FontSize="20" Foreground="CornflowerBlue" />
													</c:StretchPanel>
													<c:StretchPanel Orientation="Horizontal" c:StretchPanel.Length="*">
														<c:StretchPanel Margin="5,0,5,5" MinWidth="200">
															<TextBlock Text=" " />
															<c:StretchPanel Orientation="Horizontal">
																<TextBlock Text="Total People: " />
																<TextBlock Text="{Binding PeopleTotal}" Foreground="CornflowerBlue" />
