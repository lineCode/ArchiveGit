																<TextBlock Text="{Binding CostTotal, Converter={StaticResource StringFormatter}, ConverterParameter='\{0:c}'}" Foreground="CornflowerBlue" />
															</c:StretchPanel>
															<TextBlock Text=" " />
															<TextBlock Text="Cost Breakdown" FontSize="14" FontWeight="Bold" />
															<TextBlock Text=" " />
															<ItemsControl ItemsSource="{Binding CostData}" IsTabStop="False">
																<ItemsControl.ItemTemplate>
																	<DataTemplate>
																		<c:StretchPanel Orientation="Horizontal">
																			<TextBlock Style="{StaticResource CommonTextBlock}" Text="{Binding Name, Converter={StaticResource StringFormatter}, ConverterParameter='\{0}: '}" />
																			<TextBlock Style="{StaticResource CommonTextBlock}" Text="{Binding Value, Converter={StaticResource StringFormatter}, ConverterParameter='\{0:c}'}" Foreground="CornflowerBlue" />
																		</c:StretchPanel>
																	</DataTemplate>
																</ItemsControl.ItemTemplate>
															</ItemsControl>
														</c:StretchPanel>
														<c:StretchPanel Margin="0,5,5,5" c:StretchPanel.Length="*">
															<Charting:Chart LegendStyle="{StaticResource HiddenLegend}" c:StretchPanel.Length="*" MinWidth="100" MinHeight="100">
																<Charting:Chart.Series>
																	<Charting:PieSeries ItemsSource="{Binding CostData}" IndependentValueBinding="{Binding Name}" DependentValueBinding="{Binding Value}" />
																</Charting:Chart.Series>
															</Charting:Chart>
															<!--<telerikChart:RadChart ItemsSource="{Binding CostData}" Background="Transparent" c:StretchPanel.Length="*" />-->
														</c:StretchPanel>
													</c:StretchPanel>
												</c:StretchPanel>
											</Grid>
											<Grid Visibility="{Binding IsChecked, ElementName=x_InvitationsButton, Converter={StaticResource BoolToVisible}}">
												<c:StretchPanel>
													<c:StretchPanel Orientation="Horizontal" Margin="5">
														<TextBlock Text="Invitations Reporting: " FontSize="18" />
														<TextBlock Text="{Binding Name}" FontSize="20" Foreground="CornflowerBlue" />
													</c:StretchPanel>
													<c:StretchPanel Orientation="Horizontal" c:StretchPanel.Length="*">
														<c:StretchPanel Margin="5,0,5,5" MinWidth="200">
															<TextBlock Text=" " />
															<ItemsControl ItemsSource="{Binding InvitationsData}" IsTabStop="False">
																<ItemsControl.ItemTemplate>
																	<DataTemplate>
																		<c:StretchPanel>
																			<c:StretchPanel Orientation="Horizontal">
																				<TextBlock Style="{StaticResource CommonTextBlock}" Text="{Binding Name, Converter={StaticResource StringFormatter}, ConverterParameter='\{0}: '}" />
																				<TextBlock Style="{StaticResource CommonTextBlock}" Text="{Binding Sent}" Foreground="CornflowerBlue" />
																			</c:StretchPanel>
																			<c:StretchPanel Orientation="Horizontal">
																				<TextBlock Style="{StaticResource CommonTextBlock}" Text="Conversions: " />
																				<TextBlock Style="{StaticResource CommonTextBlock}" Text="{Binding Converted}" Foreground="CornflowerBlue" />
																			</c:StretchPanel>
																			<c:StretchPanel Orientation="Horizontal">
																				<TextBlock Style="{StaticResource CommonTextBlock}" Text="Conversion Rate: " />
																				<TextBlock Style="{StaticResource CommonTextBlock}" Text="{Binding Rate, Converter={StaticResource StringFormatter}, ConverterParameter='\{0:n2}%'}" Foreground="CornflowerBlue" />
																			</c:StretchPanel>
																			<TextBlock Text=" " />
																		</c:StretchPanel>
																	</DataTemplate>
																</ItemsControl.ItemTemplate>
															</ItemsControl>
														</c:StretchPanel>
														<c:StretchPanel Margin="0,5,5,5" c:StretchPanel.Length="*">
															<Charting:Chart LegendStyle="{StaticResource HiddenLegend}" c:StretchPanel.Length="*" MinWidth="100" MinHeight="100">
																<Charting:Chart.Series>
																	<Charting:ColumnSeries ItemsSource="{Binding InvitationsData}" IndependentValueBinding="{Binding Name}" DependentValueBinding="{Binding Sent}" />
																</Charting:Chart.Series>
															</Charting:Chart>
															<Charting:Chart LegendStyle="{StaticResource HiddenLegend}" c:StretchPanel.Length="*" MinWidth="100" MinHeight="100">
																<Charting:Chart.Series>
																	<Charting:ColumnSeries ItemsSource="{Binding InvitationsData}" IndependentValueBinding="{Binding ShortName}" DependentValueBinding="{Binding Converted}" />
																</Charting:Chart.Series>
															</Charting:Chart>
